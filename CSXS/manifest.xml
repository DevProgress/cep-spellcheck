<?xml version="1.0" encoding="UTF-8"?>
<ExtensionManifest Version="4.0" ExtensionBundleId="com.example.spellcheck" ExtensionBundleVersion="1.0"
ExtensionBundleName="Spell Check" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<ExtensionList>
		<Extension Id="com.example.spellcheck" Version="1.0" />
	</ExtensionList>
	<ExecutionEnvironment>
		<HostList>
			<Host Name="PPRO" Version="[9.1,10.9]" />
		</HostList>
		<LocaleList>
			<Locale Code="All" />
		</LocaleList>
		<RequiredRuntimeList>
			<RequiredRuntime Name="CSXS" Version="6.0" />
		</RequiredRuntimeList>
	</ExecutionEnvironment>

	<DispatchInfoList>
		<Extension Id="com.example.spellcheck">
			<DispatchInfo >
				<Resources>
					<MainPath>./index.html</MainPath>
					
				</Resources>
				
				<UI>
					<Type>Panel</Type>
					<Menu>Spellcheck Textbox</Menu>
					<Geometry>
						<Size>
							<Height>400</Height>
							<Width>550</Width>
						</Size>
					</Geometry>
				</UI>
			</DispatchInfo>
		</Extension>
	</DispatchInfoList>
</ExtensionManifest>
